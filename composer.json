{
    "name": "jakubboucek/openwhisk-runtime",
    "description": "OpenWhisk runtime tools for PHP, eg. with DigitalOcean Serverless Functions",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "digitalocean functions",
        "digitalocean serverless",
        "openwhisk",
        "openwhisk runtime"
    ],
    "authors": [
        {
            "name": "Jakub BoucÌŒek",
            "email": "pan@jakubboucek.cz"
        }
    ],
    "require": {
        "php": "^8.2",
        "psr/http-message": "^1.0 || ^2.0"
    },
    "require-dev": {
        "phpstan/phpstan": "1.10.33"
    },
    "autoload": {
        "psr-4": {
            "JakubBoucek\\OpenWhisk\\Runtime\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "phpstan": "phpstan analyse src --level 5"
    }
}
